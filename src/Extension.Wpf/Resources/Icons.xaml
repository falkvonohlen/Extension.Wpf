                        Value="{StaticResource InformationImage}" />
            </DataTrigger>
            <DataTrigger Binding="{Binding Level}"
                         Value="{x:Static logging:MappedLogLevel.Warning}">
                <Setter Property="Source"
                        Value="{StaticResource WarningImage}" />
            </DataTrigger>
            <DataTrigger Binding="{Binding Level}"
                         Value="{x:Static logging:MappedLogLevel.Error}">
                <Setter Property="Source"
                        Value="{StaticResource ErrorImage}" />
            </DataTrigger>
            <DataTrigger Binding="{Binding Level}"
                         Value="{x:Static logging:MappedLogLevel.Critical}">
                <Setter Property="Source"
                        Value="{StaticResource CriticalImage}" />
            </DataTrigger>
            <DataTrigger Binding="{Binding Level}"
                         Value="{x:Static logging:MappedLogLevel.None}">
                <Setter Property="Source"
                        Value="{StaticResource NoneImage}" />
            </DataTrigger>
        </Style.Triggers>
