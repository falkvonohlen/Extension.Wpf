    <Style x:Key="LogImageStyle" TargetType="Image">
        <Style.Triggers>
            <DataTrigger Binding="{Binding Level}"
                         Value="{x:Static logging:MappedLogLevel.Trace}">
                <Setter Property="Source"
                        Value="{StaticResource TraceImage}" />
            </DataTrigger>
            <DataTrigger Binding="{Binding Level}"
                         Value="{x:Static logging:MappedLogLevel.Debug}">
                <Setter Property="Source"
                        Value="{StaticResource DebugImage}" />
            </DataTrigger>
            <DataTrigger Binding="{Binding Level}"
                         Value="{x:Static logging:MappedLogLevel.Information}">
